   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "     country  date  PIB_per_capita  esperanza_vida  usuarios_internet\n",
      "0   Colombia  2023     6947.359287             NaN               77.3\n",
      "1   Colombia  2022     6674.596019          73.659               72.8\n",
      "64    Mexico  2023    13790.024343             NaN               81.2\n",
      "65    Mexico  2022    11385.407076          74.832               78.6\n"
     ]
