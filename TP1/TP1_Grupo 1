   "source": [
    "# Resolver ac√°\n",
    "# Paso 1: Importar las librer√≠as necesarias\n",
    "import wbdata\n",
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "\n",
    "# Paso 2: Seleccionar indicadores visibles y relevantes\n",
    "# Verificados previamente en https://data.worldbank.org/indicator\n",
    "indicadores = {\n",
    "    'NY.GDP.PCAP.CD': 'PIB_per_capita',  # PIB per c√°pita (US$ actuales) - Siempre disponible\n",
    "    'SP.DYN.LE00.IN': 'esperanza_vida',   # Esperanza de vida al nacer (a√±os) - Siempre disponible\n",
    "    'IT.NET.USER.ZS': 'usuarios_internet' # Usuarios de Internet (% poblaci√≥n) - Interesante para comparar\n",
    "}\n",
    "\n",
    "# Paso 3: Seleccionar pa√≠ses (c√≥digos ISO de 3 letras)\n",
    "paises = ['MEX', 'COL']  # M√©xico y Colombia\n",
    "\n",
    "# Paso 4: Obtener datos de la API (con manejo de errores)\n",
    "try:\n",
    "    datos = wbdata.get_dataframe(indicadores, country=paises)\n",
    "except Exception as e:\n",
    "    datos = pd.DataFrame()  # DataFrame vac√≠o si hay error\n",
    "\n",
    "# Paso 5: Convertir a DataFrame y mostrar vista preliminar\n",
    "if not datos.empty:\n",
    "    df = pd.DataFrame(datos)\n",
    "    df.reset_index(inplace=True)  # Convertir el √≠ndice en columnas normales\n",
    "    print(df.groupby('country').head(2))  # Muestra 2 registros por pa√≠s\n",
    "else:\n",
    "    print(\"No se pudieron obtener datos\")"
   ]
