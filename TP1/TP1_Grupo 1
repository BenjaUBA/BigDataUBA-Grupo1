    "# Resolver ac√°\n",
    "# Paso 1: Verificar que tenemos datos\n",
    "if not df.empty:\n",
    "    print(\"\\nüìä ESTAD√çSTICAS DESCRIPTIVAS POR PA√çS E INDICADOR\\n\")\n",
    "    \n",
    "    # Paso 2: Configurar pandas para mostrar m√°s decimales\n",
    "    pd.set_option('display.float_format', '{:.2f}'.format)\n",
    "    \n",
    "    # Paso 3: Estad√≠sticas generales\n",
    "    print(\"üîπ Estad√≠sticas generales para todos los datos:\")\n",
    "    print(df.describe())\n",
    "    \n",
    "    # Paso 4: Estad√≠sticas por pa√≠s\n",
    "    for pais in df['country'].unique():\n",
    "        print(f\"\\nüîπ Estad√≠sticas para {pais}:\")\n",
    "        print(df[df['country'] == pais].describe())\n",
    "    \n",
    "    # Paso 5: Comparaci√≥n directa de promedios\n",
    "    print(\"\\nüîπ COMPARACI√ìN DIRECTA (PROMEDIOS):\")\n",
    "    comparacion = df.groupby('country').mean(numeric_only=True)\n",
    "    print(comparacion)\n",
    "    \n",
    "    # Paso 6: Diferencia porcentual entre pa√≠ses\n",
    "    print(\"\\nüîπ DIFERENCIA PORCENTUAL (MEX/COL - 1):\")\n",
    "    diff = (comparacion.loc['Mexico'] / comparacion.loc['Colombia'] - 1) * 100\n",
    "    print(diff.round(2).astype(str) + \" %\")"
