    "# Resolver ac√° estilo pyplot\n",
    "# Filtramos datos para el a√±o m√°s reciente disponible\n",
    "df_reciente = df[df['date'] == df['date'].max()]\n",
    "\n",
    "if not df_reciente.empty:\n",
    "    print(\"\\nüìà GR√ÅFICO PIB PER C√ÅPITA (ESTILO PYPLOT)\")\n",
    "    \n",
    "    # Configurar estilo\n",
    "    plt.style.use('ggplot')\n",
    "    plt.figure(figsize=(10, 5))\n",
    "    \n",
    "    # Gr√°fico de barras\n",
    "    plt.bar(df_reciente['country'], df_reciente['PIB_per_capita'], \n",
    "            color=['#1f77b4', '#ff7f0e'], alpha=0.7)\n",
    "    \n",
    "    # Personalizaci√≥n\n",
    "    plt.title('Comparaci√≥n de PIB per c√°pita (2020)\\nM√©xico vs Colombia', pad=20)\n",
    "    plt.xlabel('Pa√≠s')\n",
    "    plt.ylabel('US$')\n",
    "    plt.grid(axis='y', linestyle='--', alpha=0.5)\n",
    "    \n",
    "    # A√±adir etiquetas de valor\n",
    "    for i, valor in enumerate(df_reciente['PIB_per_capita']):\n",
    "        plt.text(i, valor + 500, f\"${valor:,.0f}\", ha='center', va='bottom')\n",
    "    \n",
    "    plt.tight_layout()\n",
    "    plt.show()\n",
    "else:\n",
    "    print(\"No hay datos recientes para graficar.\")"
   ]
